{
	"name": "limelight/api-wrapper",
	"type": "library",
	"description": "The Limelight API wrapper provides PHP bindings for the LL API.",
	"keywords": ["framework", "api", "http", "rest", "web service", "client"],
	"homepage": "https://limelightgaming.net/",
	"license": "MIT",
	"authors": [
		{
			"name": "John Internet",
			"email": "internet@limelightgaming.net",
			"homepage": "https://github.com/JoshPiper/"
		}
	],
	"require": {
		"php": ">=7.3",
		"guzzlehttp/guzzle": "~6.0",
		"xpaw/steamid": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8"
	},
	"autoload": {
		"psr-4": {
			"Limelight\\API\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Limelight\\API\\Tests\\": "tests/"
		}
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/kanalumaddela/SteamID.php.git"
		}
	]
}
